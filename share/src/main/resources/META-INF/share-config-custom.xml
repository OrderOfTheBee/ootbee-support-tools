<?xml version='1.0' encoding='UTF-8'?>
<alfresco-config>
    <config evaluator="string-compare" condition="WebFramework">
        <web-framework>
            <dojo-pages>
                <packages>
                    <package name="ootbee-support-tools" location="ootbee-support-tools" />
                </packages>
            </dojo-pages>
        </web-framework>
    </config>

    <config evaluator="string-compare" condition="DocLibCustom">
        <dependencies>
            <js src="fme/components/jsconsole/jsconsole-actions.js" />
        </dependencies>
    </config>

    <config evaluator="string-compare" condition="DocLibActions">
        <actions>
            <action id="de_fme_jsconsole-open-in-jsconsole" type="javascript"
                label="fme.action.open.in.jsconsole" icon="open-in-jsconsole">
                <param name="function">onActionOpenInJavascriptConsole</param>
                <evaluator>fme.evaluator.doclib.action.isAdmin</evaluator>
            </action>
            <action id="de_fme_jsconsole-dump-in-jsconsole" type="javascript"
                label="fme.action.dump.in.jsconsole" icon="dump-in-jsconsole">
                <param name="function">onActionDumpInJavascriptConsole</param>
                <evaluator>fme.evaluator.doclib.action.isAdmin</evaluator>
            </action>
        </actions>

        <actionGroups>
            <actionGroup id="document-browse">
                <action index="400" id="de_fme_jsconsole-open-in-jsconsole" />
                <action index="401" id="de_fme_jsconsole-dump-in-jsconsole" />
            </actionGroup>
            <actionGroup id="document-details">
                <action index="400" id="de_fme_jsconsole-open-in-jsconsole" />
                <action index="401" id="de_fme_jsconsole-dump-in-jsconsole" />
            </actionGroup>
            <actionGroup id="folder-browse">
                <action index="400" id="de_fme_jsconsole-open-in-jsconsole" />
                <action index="401" id="de_fme_jsconsole-dump-in-jsconsole" />
            </actionGroup>
            <actionGroup id="folder-details">
                <action index="400" id="de_fme_jsconsole-open-in-jsconsole" />
                <action index="401" id="de_fme_jsconsole-dump-in-jsconsole" />
            </actionGroup>
        </actionGroups>
    </config>
</alfresco-config>