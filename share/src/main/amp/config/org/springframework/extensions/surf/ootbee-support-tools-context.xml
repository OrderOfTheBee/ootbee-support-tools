<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean class="org.springframework.extensions.surf.util.ResourceBundleBootstrapComponent">
        <property name="resourceBundles">
            <list>
                <value>alfresco.messages.ootbee-support-tools</value>
            </list>
        </property>
    </bean>
    
    <bean id="webscript.org.orderofthebee.pages.console.support-tools.log4j-loggers.get" class="org.orderofthebee.addons.support.tools.share.LogStateWebScript"
        parent="webscript" />
    <bean id="webscript.org.orderofthebee.pages.console.support-tools.log4j-loggers.put" class="org.orderofthebee.addons.support.tools.share.LogStateWebScript"
        parent="webscript" />
    <bean id="webscript.org.orderofthebee.pages.console.support-tools.log4j-loggers.delete" class="org.orderofthebee.addons.support.tools.share.LogStateWebScript"
        parent="webscript" />

    <bean id="webscript.org.orderofthebee.pages.console.support-tools.log4j-log-file.get" class="org.orderofthebee.addons.support.tools.share.LogFileGet"
        parent="webscript" />

    <bean id="webscript.org.orderofthebee.pages.console.support-tools.log4j-log-file.delete" class="org.orderofthebee.addons.support.tools.share.LogFileDelete"
        parent="webscript" />

    <bean id="webscript.org.orderofthebee.pages.console.support-tools.log4j-log-files.post" class="org.orderofthebee.addons.support.tools.share.LogFilesZIPPost"
        parent="webscript" />

</beans>