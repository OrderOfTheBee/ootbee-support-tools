<?xml version="1.0" encoding="UTF-8" ?>
<webscript>
    <shortname>Log4J Settings</shortname>
    <description>View and update Log4J settings</description>
    <url>/ootbee/admin/log4j-settings</url>
    <family>AdminConsole</family>
    <!-- Enterprise console does not handle tool URL + grouping properly - so restrict to Community only -->
    <family>AdminConsole:Edition:Community</family>
    <format default="html">argument</format>
    <authentication>admin</authentication>
    <lifecycle>internal</lifecycle>
    <transaction allow="readonly">required</transaction>
</webscript>